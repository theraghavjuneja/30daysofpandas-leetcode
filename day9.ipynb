    "import pandas as pd\n",
    "\n",
    "def find_patients(patients: pd.DataFrame) -> pd.DataFrame:\n",
    "    return patients[patients['conditions'].str.contains(r'/bDIAB1',regex=True)]\n",
    "    # /b means it is the word boundary as simple as that"
